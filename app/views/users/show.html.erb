<% provide(:title, 'User') %>
<% if logged_in? %>
	<div class="row">
		<aside class="col-sm-4">
			<section class="user_info">
				<%= render 'shared/user_info' %>
			</section>
		</aside>
		<aside class="col-sm-8">
			<div class="row">
				<div class="col-md-12">
					<section class="user_feed">
						<h3>Your Products</h3>
						<%= render 'shared/feed' %>
					</section>
				</div>
			</div>
			<div class="row">
				<div class="col-md-12">
					<section class="user_info">
						<h3>Your Wishlist</h3>
						
					</section>
				</div>
			</div>
			<div class="row">
				<div class="col-md-12">
					<section class="user_info">
						<h3>Your History</h3>
					</section>
				</div>
			</div>
		</aside>
	</div>
<% else %>
	Sorry, you do not have access to this page. Please login to view this page. 
<% end %>